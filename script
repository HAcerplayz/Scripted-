ask.spawn(function()
	if not makefolder then
		LocalPlayer:Kick("Your executor does not support AlSploit. Switch to Delta or Fluxus")
	end
 
	if not writefile then
		LocalPlayer:Kick("Your executor does not support AlSploit. Switch to Delta or Fluxus")
	end
 
	if not readfile then
		LocalPlayer:Kick("Your executor does not support AlSploit. Switch to Delta or Fluxus")
	end
 
	if not isfile then
		LocalPlayer:Kick("Your executor does not support AlSploit. Switch to Delta or Fluxus")
	end
